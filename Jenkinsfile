pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building Code..'
            }
        }
        stage('Unit Test') {
            steps {
                echo 'Unit Tests are running..,'
            }
        }
        stage('Functionel Tets') {
            steps {
                echo 'Functional Tests are running'
            }
        }
        stage('Release Process') {
            steps {
                echo 'Release Process...'
            }
        }
        stage('Deployment') {
            steps {
                echo 'Deployment done!!!'
            }
        }
    }
}
